      "type" : "expression",
      "value" : [
        "var date2 = new Date();\r",
        "\r",
        "if ($data.confirmationFabrication.dateHeureRecup !== undefined){\r",
        "\r",
        "    var dateLim = $data.confirmationFabrication.dateHeureRecup.split(/\\D+/);\r",
        "    date2 = new Date(Date.UTC(dateLim[0], --dateLim[1], dateLim[2], dateLim[3], dateLim[4], dateLim[5], dateLim[6]));\r",
        "\r",
        "    date2.setTime(date2.getTime() - (2*60*60*1000));\r",
        "    \r",
        "    console.log(date2.toISOString());\r",
        "    \r",
        "}\r",
        "\r",
