    "qteRenvoye" : {
      "type" : "expression",
      "value" : [
        "if ($data.confirmationFabrication !== undefined){",
        "    var qte;",
        "    if ($data.task.displayDescription == 'choco'){",
        "        qte = $data.confirmationFabrication.qteChoco;",
        "    }",
        "    if ($data.task.displayDescription == 'coco'){",
        "        qte = $data.confirmationFabrication.qteCoco;",
        "    }",
